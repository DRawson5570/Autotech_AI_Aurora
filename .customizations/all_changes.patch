+
+class UserTokenUsageModel(BaseModel):
+    id: str
+    user_id: str
+    period_start: int
+    period_end: int
+    tokens_prompt: int
+    tokens_completion: int
+    tokens_total: int
+    cost_total: str | None = None
+    currency: str | None = "USD"
